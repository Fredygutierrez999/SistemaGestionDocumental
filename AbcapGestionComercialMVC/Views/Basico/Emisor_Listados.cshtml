@using GestionDocumental.Models.Clases.Radicacion
@model List<clsAppNetEmisor>
@if (Model.Count > 0)
{
    <table class="table table-bordered table-hover table-condensed table-responsive">
        <thead>
            <tr>
                <td>
                    <label>Id</label>
                </td>
                <td>
                    <label>Número</label>
                </td>
                <td>
                    <label>Nombre</label>
                </td>
            </tr>
        </thead>
        <tbody>
            @foreach (clsAppNetEmisor objItem in Model)
            {
                <tr>
                    <td>
                        <a href="javascript:AministrarEmisor(@objItem.ID)">@objItem.ID</a>
                    </td>
                    <td>@objItem.Numero</td>
                    <td>@objItem.Nombre</td>
                </tr>
            }
        </tbody>
        <tbody></tbody>
    </table>
}
else
{
    <div class="row">
        <div class="col-md-12">
            <div class="alert alert-info">
                Sin registros
            </div>
        </div>
    </div>
}